datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id              String  @id @default(cuid())
  name            String  @default("")
  email           String  @unique @default("")
  password        String
  isAdmin         Boolean @default(false)
  from_Order_user Order[] @relation("Order_user")
}

model Category {
  id                    String    @id @default(cuid())
  name                  String    @default("")
  valid                 Boolean   @default(false)
  from_Product_category Product[] @relation("Product_category")
}

model Product {
  id                 String    @id @default(cuid())
  name               String    @default("")
  price              String    @default("")
  category           Category? @relation("Product_category", fields: [categoryId], references: [id])
  categoryId         String?   @map("category")
  from_Order_product Order[]   @relation("Order_product")

  @@index([categoryId])
}

model Order {
  id        String   @id @default(cuid())
  user      User?    @relation("Order_user", fields: [userId], references: [id])
  userId    String?  @map("user")
  product   Product? @relation("Order_product", fields: [productId], references: [id])
  productId String?  @map("product")

  @@index([userId])
  @@index([productId])
}
